{
  "name": "calc-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:func": "react-scripts test func.test hook.test",
    "test:visual": "react-scripts test visual.test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:func:coverage": "react-scripts test --watchAll=false --coverage func.test hook.test",
    "test:visual:coverage": "react-scripts test --watchAll=false --coverage visual.test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "formik": "2.2.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.39.0",
    "styled-components": "5.3.6",
    "web-vitals": "2.1.4",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/faker": "5.5.9",
    "@types/jest": "27.4.0",
    "@types/jest-image-snapshot": "4.3.1",
    "@types/jsdom-screenshot": "3.2.1",
    "@types/node": "16.11.22",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/styled-components": "5.1.26",
    "date-fns": "2.28.0",
    "faker": "5.5.3",
    "jest-image-snapshot": "4.5.1",
    "jest-transform-css": "4.0.0",
    "jest-transform-file": "1.1.1",
    "jest-watch-typeahead": "1.0.0",
    "jest-websocket-mock": "2.3.0",
    "jsdom-screenshot": "4.0.0",
    "nock": "13.2.4",
    "react-scripts": "5.0.0",
    "typescript": "4.6.4"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/reportWebVitals.ts"
    ]
  }
}
