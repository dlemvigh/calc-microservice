{
  "name": "calc-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:func": "react-scripts test func.test hook.test",
    "test:visual": "react-scripts test visual.test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:func:coverage": "react-scripts test --watchAll=false --coverage func.test hook.test",
    "test:visual:coverage": "react-scripts test --watchAll=false --coverage visual.test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "formik": "2.2.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.32.1",
    "styled-components": "5.3.3",
    "web-vitals": "2.1.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/faker": "5.5.9",
    "@types/jest": "27.0.2",
    "@types/jest-image-snapshot": "4.3.1",
    "@types/jsdom-screenshot": "3.2.1",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.15",
    "date-fns": "2.25.0",
    "faker": "5.5.3",
    "jest-image-snapshot": "4.5.1",
    "jest-transform-css": "2.1.0",
    "jest-transform-file": "1.1.1",
    "jest-websocket-mock": "2.2.1",
    "jsdom-screenshot": "4.0.0",
    "nock": "13.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.4.4"
  },
  "volta": {
    "node": "16.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/reportWebVitals.ts"
    ]
  }
}
