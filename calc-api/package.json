{
  "name": "calc-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "test": "mocha tests/**/*.test.ts",
    "test:coverage": "nyc --reporter=lcov --reporter=text mocha tests/**/*.test.ts",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "mocha": {
    "diff": true,
    "require": [
      "ts-node/register",
      "./tests/common.ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "tests/**/*.ts"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ws": "^8.2.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@tsconfig/node14": "1.0.1",
    "@types/chai": "4.2.22",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/mocha": "9.0.0",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "3.2.5",
    "@types/ws": "8.2.0",
    "chai": "4.3.4",
    "faker": "5.5.3",
    "mocha": "9.1.3",
    "nodemon": "2.0.13",
    "nyc": "15.1.0",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "volta": {
    "node": "16.13.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/aws/sqs.ts",
      "src/config.ts",
      "src/index.ts"
    ]
  }
}
