{
  "name": "calc-worker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "test": "mocha tests/**/*.test.ts",
    "test:coverage": "nyc --reporter=lcov --reporter=text mocha tests/**/*.test.ts",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "mocha": {
    "diff": true,
    "require": [
      "ts-node/register",
      "./tests/common.ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "tests/**/*.ts"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "node-fetch": "^2.6.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@tsconfig/node16": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.22",
    "@types/node-fetch": "2.5.12",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "3.2.8",
    "chai": "4.3.4",
    "mocha": "9.1.3",
    "nock": "13.2.1",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.3.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/aws/sqs.ts",
      "src/config.ts",
      "src/index.ts"
